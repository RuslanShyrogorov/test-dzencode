{
  "extends":  ["next/core-web-vitals","prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier/recommended"
  ],
  "rules": {
    "comma-dangle": [2, "always-multiline"]
  }
}
